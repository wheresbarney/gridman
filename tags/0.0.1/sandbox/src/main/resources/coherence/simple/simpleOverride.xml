<?xml version="1.0"?>
<!DOCTYPE coherence SYSTEM "coherence.dtd">
<coherence>
    <cluster-config>
        <!-- This changes the timeout from the default of 30s -->
        <multicast-listener>
            <join-timeout-milliseconds system-property="coherence.incubator.joinTimeout">1000</join-timeout-milliseconds>
        </multicast-listener>
    </cluster-config>
</coherence>